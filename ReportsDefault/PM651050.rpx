    <Detail Name="detailSection1">
      <Height>0.42328cm</Height>
      <Variables>
        <ReportVariable>
          <Name>ThroughDate</Name>
          <ValueExpr>=CStr(CInt(Left([@To_Date],4))-1) + '12'</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>ActualCostAmt</Name>
          <ValueExpr>=$ActualCostAmt + IIf([PMWipTotalForecastHistory.PeriodID]&gt;=@From_Date and [PMWipTotalForecastHistory.PeriodID]&lt;=@To_Date, [PMWipTotalForecastHistory.ActualCostAmount],0)</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>ActualRevenueAmt</Name>
          <ValueExpr>=$ActualRevenueAmt + IIf([PMWipTotalForecastHistory.PeriodID]&gt;=@From_Date and [PMWipTotalForecastHistory.PeriodID]&lt;=@To_Date, [PMWipTotalForecastHistory.ActualRevenueAmount] + [PMWipTotalForecastHistory.ArRevenueInclTaxAmount],0)#010;</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>ActualCostAmtTodate</Name>
          <ValueExpr>=$ActualCostAmtTodate + IIF([PMWipTotalForecastHistory.PeriodID] &lt;= [@ActToPeriod],[PMWipTotalForecastHistory.ActualCostAmount], 0)</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>ActualRevenueAmtTodate</Name>
          <ValueExpr>=$ActualRevenueAmtTodate +  IIF([PMWipTotalForecastHistory.PeriodID] &lt;= [@To_Date],[PMWipTotalForecastHistory.ActualRevenueAmount] + [PMWipTotalForecastHistory.ArRevenueInclTaxAmount], 0)#010;</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>CostThroughAmt</Name>
          <ValueExpr>=$CostThroughAmt + IIf([PMWipTotalForecastHistory.PeriodID]&lt;=$ThroughDate, [PMWipTotalForecastHistory.ActualCostAmount],0)#010;</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>BilledThroughAmt</Name>
          <ValueExpr>=$BilledThroughAmt + IIf([PMWipTotalForecastHistory.PeriodID]&lt;=$ThroughDate, [PMWipTotalForecastHistory.ActualRevenueAmount] + [PMWipTotalForecastHistory.ArRevenueInclTaxAmount],0)</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>ArRevenueAmt</Name>
          <ValueExpr>=$ArRevenueAmt + IIf([PMWipTotalForecastHistory.PeriodID]&gt;=@From_Date and [PMWipTotalForecastHistory.PeriodID]&lt;=@To_Date,[PMWipTotalForecastHistory.ArRevenueAmount] + [PMWipTotalForecastHistory.ArRevenueInclTaxAmount] ,0)</ValueExpr>
        </ReportVariable>
        <ReportVariable>
          <Name>ArRevenueAmtTodate</Name>
          <ValueExpr>=$ArRevenueAmtTodate + IIf([PMWipTotalForecastHistory.PeriodID]&lt;=@ActToPeriod, [PMWipTotalForecastHistory.ArRevenueAmount] + [PMWipTotalForecastHistory.ArRevenueInclTaxAmount], 0)</ValueExpr>
        </ReportVariable>
		<ReportVariable>
		  <Name>ArAssetAmt</Name>
		  <ValueExpr>=$ArAssetAmt + IIf([PMWipTotalForecastHistory.PeriodID]&gt;=@From_Date and [PMWipTotalForecastHistory.PeriodID]&lt;=@To_Date,[PMWipTotalForecastHistory.ArAssetAmount] ,0)</ValueExpr>
		</ReportVariable>
		<ReportVariable>
		  <Name>ArAssetAmtTodate</Name>
		  <ValueExpr>=$ArAssetAmtTodate + IIf([PMWipTotalForecastHistory.PeriodID]&lt;=@ActToPeriod, [PMWipTotalForecastHistory.ArAssetAmount], 0)</ValueExpr>
